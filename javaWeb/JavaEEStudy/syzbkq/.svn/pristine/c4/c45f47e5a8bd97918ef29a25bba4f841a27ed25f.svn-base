package com.neuedu.vacation.entity;

import java.util.Date;

import cn.thinking.common.page.Page;

public class Vacation {

	private int vacationId;			//假期ID
	private String vacationName;	//假期名称
	private String vacationExplain;	//假期说明
	private String vacationRemarks;	//假期备注
	private String vacationType;	//假期类型
	private String vacationTypeName;//假期类型名称
	private String vacationStatus;	//是否有效（0：无效，1：有效）
	private String createName;		//创建人名称
	private Date createTime;		//创建时间
	private Date updateTime;		//最后修改时间
	
	private Page page = new Page();
	
	public int getVacationId() {
		return vacationId;
	}
	public void setVacationId(int vacationId) {
		this.vacationId = vacationId;
	}
	public String getVacationName() {
		return vacationName;
	}
	public void setVacationName(String vacationName) {
		this.vacationName = vacationName;
	}
	public String getVacationExplain() {
		return vacationExplain;
	}
	public void setVacationExplain(String vacationExplain) {
		this.vacationExplain = vacationExplain;
	}
	public String getVacationRemarks() {
		return vacationRemarks;
	}
	public void setVacationRemarks(String vacationRemarks) {
		this.vacationRemarks = vacationRemarks;
	}
	public String getVacationType() {
		return vacationType;
	}
	public void setVacationType(String vacationType) {
		this.vacationType = vacationType;
	}
	public String getVacationStatus() {
		return vacationStatus;
	}
	public void setVacationStatus(String vacationStatus) {
		this.vacationStatus = vacationStatus;
	}
	public String getCreateName() {
		return createName;
	}
	public void setCreateName(String createName) {
		this.createName = createName;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	public String getVacationTypeName() {
		return vacationTypeName;
	}
	public void setVacationTypeName(String vacationTypeName) {
		this.vacationTypeName = vacationTypeName;
	}
	public Page getPage() {
		return page;
	}
	public void setPage(Page page) {
		this.page = page;
	}
	
	
}
