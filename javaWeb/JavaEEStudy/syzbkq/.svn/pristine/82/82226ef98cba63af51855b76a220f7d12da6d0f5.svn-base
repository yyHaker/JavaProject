.top{
    position: fixed;
    top: 60px;
    left: 210px;
    width: 100%;
    height: 35px;
    background-color: #F5F5F5;
    border-style: solid;
    border-width: 0;
    border-bottom-width: 1px;
    border-color: #DCDCDC;
    padding: 8px 15px;
}
input,select,label{
    font-weight: 300;
    color: #666;
}
#searchForm input, select{
		margin: 2px 0px 0px 0px;
