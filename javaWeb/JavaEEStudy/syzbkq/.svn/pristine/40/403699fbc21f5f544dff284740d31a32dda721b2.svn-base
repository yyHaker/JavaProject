package cn.thinking.org.system.trainingplan;

import java.util.Date;

public class Trainingplan {
	private String trainingplannum;//培训计划编号
	private String tobjectives ;//培训目标
	private String tcontent;//培训内容
	private String tobject;//培训对象
	private int plannumber;//计划人数
	private String planperiod;//计划课时
	private Date trainingdate;//培训日期
	private String tplaces;//培训地点
	private String lecturesource;//讲师来源
	private String hostdepartment;//主办部门
	private String assistdepartment;//协助部门
	private String plancompletion;//计划完成状态
	public String getTrainingplannum() {
		return trainingplannum;
	}
	public void setTrainingplannum(String trainingplannum) {
		this.trainingplannum = trainingplannum;
	}
	public String getTobjectives() {
		return tobjectives;
	}
	public void setTobjectives(String tobjectives) {
		this.tobjectives = tobjectives;
	}
	public String getTcontent() {
		return tcontent;
	}
	public void setTcontent(String tcontent) {
		this.tcontent = tcontent;
	}
	public String getTobject() {
		return tobject;
	}
	public void setTobject(String tobject) {
		this.tobject = tobject;
	}
	public int getPlannumber() {
		return plannumber;
	}
	public void setPlannumber(int plannumber) {
		this.plannumber = plannumber;
	}
	public String getPlanperiod() {
		return planperiod;
	}
	public void setPlanperiod(String planperiod) {
		this.planperiod = planperiod;
	}
	public Date getTrainingdate() {
		return trainingdate;
	}
	public void setTrainingdate(Date trainingdate) {
		this.trainingdate = trainingdate;
	}
	public String getTplaces() {
		return tplaces;
	}
	public void setTplaces(String tplaces) {
		this.tplaces = tplaces;
	}
	public String getLecturesource() {
		return lecturesource;
	}
	public void setLecturesource(String lecturesource) {
		this.lecturesource = lecturesource;
	}
	public String getHostdepartment() {
		return hostdepartment;
	}
	public void setHostdepartment(String hostdepartment) {
		this.hostdepartment = hostdepartment;
	}
	public String getAssistdepartment() {
		return assistdepartment;
	}
	public void setAssistdepartment(String assistdepartment) {
		this.assistdepartment = assistdepartment;
	}
	public String getPlancompletion() {
		return plancompletion;
	}
	public void setPlancompletion(String plancompletion) {
		this.plancompletion = plancompletion;
	}
	public Trainingplan(String trainingplannum, String tobjectives,
			String tcontent, String tobject, int plannumber, String planperiod,
			Date trainingdate, String tplaces, String lecturesource,
			String hostdepartment, String assistdepartment,
			String plancompletion) {
		super();
		this.trainingplannum = trainingplannum;
		this.tobjectives = tobjectives;
		this.tcontent = tcontent;
		this.tobject = tobject;
		this.plannumber = plannumber;
		this.planperiod = planperiod;
		this.trainingdate = trainingdate;
		this.tplaces = tplaces;
		this.lecturesource = lecturesource;
		this.hostdepartment = hostdepartment;
		this.assistdepartment = assistdepartment;
		this.plancompletion = plancompletion;
	}
	public Trainingplan() {
		super();
		// TODO 自动生成的构造函数存根
	}
	
	

}
